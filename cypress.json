{
    "viewportWidth": 1024,
    "viewportHeight": 768,
    "defaultCommandTimeout": 10000,
    "baseUrl": "https://opentdb.com/",
    "reporter": "mochawesome",
	"reporterOptions": {
		"reportDir": "cypress/results",
		"overwrite": false,
		"html": true,
		"json": false,
		"timestamp": "mmddyyyy_HHMMss",
		"reportTitle": "Relatório de Execução Teste",
		"cdn": true,
		"charts": true,
		"toConsole": true
    },
    "retries": {
		"runMode": 1,
		"openMode": 1
	}
}
